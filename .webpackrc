{
  "entry": "src/index.js",
  "extraBabelPlugins": [["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-remove-console"
      ]
    }
  },

  "publicPath": "/",
  "html": {
    "template": "./src/index.ejs"
  },
  "ignoreMomentLocale": true,
  "hash": true
}