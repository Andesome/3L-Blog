{
  "entry": "src/index.js",
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-remove-console"
      ]
    }
  },

  "publicPath": "/",
  "html": {
    "template": "./src/index.ejs"
  },
  "ignoreMomentLocale": true,
  "hash": true,
  "theme": {
    "@primary-color": "#1abc9c"
  }
}